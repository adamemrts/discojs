{
  "name": "discojs",
  "version": "1.3.2",
  "description": "Easiest way to use the Discogs API in Javascript",
  "author": "aknorw",
  "license": "MIT",
  "keywords": [
    "discogs",
    "api",
    "client"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "pretty": "prettier --config .prettierrc --write 'src/**/*.{ts,json}'",
    "lint": "eslint --ext .ts,.json src",
    "test": "cross-env NODE_ENV=test nyc --reporter=text mocha ./test ./src/**/*.test.js ./src/**/**/*.test.js --require @babel/register --require ./test/setup --globals global --timeout 10000",
    "test:ci": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "prebuild": "yarn run clean",
    "build": "cross-env NODE_ENV=production rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aknorw/discojs.git"
  },
  "homepage": "https://github.com/aknorw/discojs#readme",
  "bugs": {
    "url": "https://github.com/aknorw/discojs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,json}": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cross-fetch": "^3.0.4",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.8.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "typescript": "^3.8.3"
  }
}
