{
  "name": "discojs",
  "version": "1.3.2",
  "description": "Easiest way to use the Discogs API in Javascript",
  "author": "aknorw",
  "license": "MIT",
  "keywords": [
    "discogs",
    "api",
    "client"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "pretty": "prettier --config .prettierrc --write '{models,src}/**/*.{ts,json}'",
    "lint": "eslint --ext .ts,.json models/ src/",
    "test": "cross-env NODE_ENV=test jest",
    "docs": "typedoc --inputFiles src/index.ts --exclude \"**/*+(.spec).ts\" --excludeNotExported --excludePrivate --stripInternal",
    "prebuild": "yarn run clean",
    "build": "cross-env NODE_ENV=production rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aknorw/discojs.git"
  },
  "homepage": "https://github.com/aknorw/discojs#readme",
  "bugs": {
    "url": "https://github.com/aknorw/discojs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,json}": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cross-fetch": "^3.0.5",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@wessberg/rollup-plugin-ts": "^1.3.4",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "fp-ts": "^2.8.2",
    "husky": "^4.3.0",
    "io-ts": "^2.2.10",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-commonjs": "^10.1.0",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  }
}
